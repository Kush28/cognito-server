{
  "name": "cognito-server",
  "version": "1.0.0",
  "description": "Cognito Server",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "start": "node dist/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "prettier": "prettier --config ./.prettierrc --write \"**/*.js\"",
    "pretest": "eslint --ignore-path .gitignore .",
    "postinstall": "rm -rf dist && npm run build",
    "lint": "npm run prettier --write --check --config ./.prettierrc \"**/*.js\" && eslint --fix ./src",
    "release": "release-it patch --no-npm.publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kush28/cognito-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kush28/cognito-server/issues"
  },
  "homepage": "https://github.com/Kush28/cognito-server#readme",
  "dependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/node": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "apollo-datasource": "^0.7.3",
    "apollo-engine": "^1.1.2",
    "apollo-server": "^2.19.2",
    "apollo-server-express": "^2.19.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-compose": "^7.24.4",
    "graphql-compose-connection": "^8.0.1",
    "graphql-compose-mongoose": "^9.0.1",
    "graphql-middleware": "^6.0.3",
    "graphql-tools": "^7.0.2",
    "helmet": "^4.4.1",
    "http": "0.0.1-security",
    "http-errors": "^1.8.0",
    "jade": "^1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.15",
    "mongoose-bcrypt": "^1.9.0",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.19.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "fs-extra": "^9.1.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
